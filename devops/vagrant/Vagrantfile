#SEE: https://docs.vagrantup.com.

Vagrant.configure("2") do |config| #A: the "2" is for the vagrant version
  config.vm.define "dev" do |host1|
    host1.vm.box = "debian/bookworm64" #DOC: base image to start from #SEE: https://app.vagrantup.com/boxes/search
  end
  
  config.vm.network "private_network", type: "dhcp", name: "vboxnet0" #A: host only
  config.vm.disk :disk, size: "50GB", primary: true

  config.vm.provider "virtualbox" do |vb|
     vb.gui = true #U: Display the VirtualBox GUI when booting the machine
     vb.memory = "8192" #U: RAM for the VM
  end

	# -*- mode: ruby -*-
	# vi: set ft=ruby :
end
